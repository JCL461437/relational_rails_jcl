<h1>New Gym</h1>
<div id="new-gym">
  <%= form_with url: '/gyms', method: :post, local: true do |form|%>
    <%= form.label :name %>
    <%= form.text_field :name %>

    <br> 
    <%= form.label :number_of_champions %>
    <%= form.number_field :number_of_champions %>

    <br> 
    <%= form.label :holds_current_champion %>
    <%= form.text_field :holds_current_champion %>

    <br> 
    <%= form.submit "Create Gym" %>
  <% end %>
</div>